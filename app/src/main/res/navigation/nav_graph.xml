<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/todoGraph">

    <include-dynamic
        android:id="@+id/todoGraph"
        app:graphResName="todo_graph"
        app:moduleName="feature_todo" >
        <action
            android:id="@+id/action_todoGraph_to_newTodo"
            app:destination="@id/newTodo" />
    </include-dynamic>
    <fragment
        android:id="@+id/newTodo"
        android:name="com.example.feature_todo.fragments.NewTodo"
        app:moduleName="Todo.feature_todo"
        android:label="todo_new"
        tools:layout="@layout/todo_new" >
        <action
            android:id="@+id/action_newTodo_to_todoGraph"
            app:destination="@id/todoGraph" />
    </fragment>
</navigation>